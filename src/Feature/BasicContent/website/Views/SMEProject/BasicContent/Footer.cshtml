@model SMEProject.Feature.BasicContent.Models.FooterModel
<footer class="bg-dark text-light py-1 mt-2">
    <div class="container">
        <div class="row">
            <div class="col-md-4">
                <h5>@Model.ContactTitle</h5>
                <p>
                    @Model.HotelName<br>
                    @Model.Address<br>
                    @Model.PhoneNumber<br>
                    @Model.HotelEmail
                </p>
            </div>
            <div class="col-md-4">
                <h5>@Model.ExploreTitle</h5>
                <ul class="list-unstyled">
                    <li class="nav-link"><a class="nav-item" href="@Model.Rooms">@Model.RoomPlaceholder</a></li>
                    <li class="nav-link"><a class="nav-item" href="@Model.Dinning">@Model.DinningPlaceholder</a></li>
                </ul>
            </div>
            <div class="col-md-4">
                <h5>@Model.SocialTitle</h5>
                <ul class="list-unstyled">
                    <li class="nav-link"><a href="@Model.Facebook"><i class="fab fa-facebook-f"><svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-facebook" viewBox="0 0 16 16">
    <path d="M16 8.049c0-4.446-3.582-8.05-8-8.05C3.58 0-.002 3.603-.002 8.05c0 4.017 2.926 7.347 6.75 7.951v-5.625h-2.03V8.05H6.75V6.275c0-2.017 1.195-3.131 3.022-3.131.876 0 1.791.157 1.791.157v1.98h-1.009c-.993 0-1.303.621-1.303 1.258v1.51h2.218l-.354 2.326H9.25V16c3.824-.604 6.75-3.934 6.75-7.951" />
</svg></i>@Model.FacebookPlaceholder</a></li>
                    <li class="nav-link"><a href="@Model.Twitter"><i class="fab fa-twitter">@Model.TwitterPlaceholder</a></li>
                    <li class="nav-link"><a href="@Model.Instagram"><i class="fab fa-instagram">@Model.InstagramPlaceholder</a></li>
                    
                </ul>
            </div>
        </div>
        <hr>
        <div class="text-center">
            <p>@Model.Copywright</p>
        </div>
    </div>
</footer>